<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NFPCharting.ViewCycle"
             BackgroundColor="{DynamicResource PageBackgroundColor}">
    <ContentPage.Content>
        <StackLayout>
            <WebView x:Name="web_view" HeightRequest="1080" WidthRequest="1920" BackgroundColor="{DynamicResource PageBackgroundColor}">
            </WebView>            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>